      "contents": {
        "description": "string",
        "name": "string",
        "thumbnailUrl": "string"
      },
      "description": "a new widget message with description and thumbnailUrl",
      "generators": {
        "body": {
          "$.description": {
            "size": 20,
            "type": "RandomString"
          },
          "$.name": {
            "size": 20,
            "type": "RandomString"
          },
          "$.thumbnailUrl": {
            "size": 20,
            "type": "RandomString"
          }
        }
      },
      "matchingRules": {
        "body": {
          "$.description": {
            "combine": "AND",
            "matchers": [
              {
                "match": "type"
              }
            ]
          },
          "$.name": {
            "combine": "AND",
            "matchers": [
              {
                "match": "type"
              }
            ]
          },
          "$.thumbnailUrl": {
            "combine": "AND",
            "matchers": [
              {
                "match": "type"
              }
            ]
          }
        }
      },
      "metaData": {
        "contentType": "application/json"
      }
    },
